CREATE TABLE CONFIGURACIONES (ID INTEGER,DES_CONFIGURACION VARCHAR(255))
CREATE TABLE CONFIG_COLORES (ID_CONFIG INTEGER, ACCION VARCHAR(15), COLOR_HEXA VARCHAR(15), COLOR_HSSF VARCHAR(15))
CREATE TABLE TABLAS (ID_CONFIG INTEGER, PESTAGNA VARCHAR(100), NOMBRE_TABLA VARCHAR(100),PK_TABLA VARCHAR(100), LINEA_COLUMNAS INTEGER, LINEA_BLOQUEOS INTEGER, LINEA_DATOS INTEGER, COLUMNA_DATOS INTEGER)
CREATE TABLE ACCIONES (ID_CONFIG INTEGER, NOMBRE_TABLA VARCHAR(100), PESTAGNA VARCHAR(100), ACCION VARCHAR(10))
CREATE TABLE BINARIAS (ID_CONFIG INTEGER, NOMBRE_TABLA VARCHAR(100), PESTAGNA VARCHAR(100), COLUMNA_BINARIA VARCHAR(100))
CREATE TABLE TRADUCCIONES (ID_CONFIG INTEGER, NOMBRE_TABLA VARCHAR(100), PESTAGNA VARCHAR(100), COLUMNA_EXCEL VARCHAR(100), COLUMNA_BBDD VARCHAR(100))
CREATE TABLE DATOS_SIMPLIFICADOS (ID_CONFIG INTEGER, NOMBRE_TABLA VARCHAR(100), PESTAGNA VARCHAR(100), COLUMNA_SIMPLIFICADA VARCHAR(100), COLUMNA VARCHAR(100))
CREATE TABLE DATOS_PARAMETRIZADOS (ID_CONFIG INTEGER, NOMBRE_TABLA VARCHAR(100), PESTAGNA VARCHAR(100), COLUMNA_PARAMETRIZADA VARCHAR(100), DATO VARCHAR(100), TRADUCCION VARCHAR(100))
CREATE TABLE CAMPOS_TIPO (ID_CONFIG INTEGER, NOMBRE_TABLA VARCHAR(100), CAMPO VARCHAR(100), TIPO VARCHAR (50))